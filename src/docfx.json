{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": ".."
        }
      ],
      "dest": "../docs/api",
      "properties": {
        "TargetFramework": "netstandard1.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/api/**.yml",
          "docs/api/index.md"
        ],
        "src": ".."
      },
      {
        "files": [
          "docs/articles/**.md",
          "docs/articles/**/toc.yml",
          "docs/toc.yml",
          "docs/*.md"
        ],
        "src": ".."
      }
    ],
    "resource": [
      {
        "files": [
          "docs/images/**"
        ],
        "src": ".."
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/_site/**"
        ],
        "src": ".."
      }
    ],
    "dest": "../docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}